    <control id="menuBar" type="menuManager">
      <item label="ファイル(&amp;F)">
        <item>newAction</item>
        <!-- ラベルが空のitemはセパレータを表します -->
        <item label="" />
        <item>openAction</item>
        <item>closeAction</item>
        <item label="" />
        <item>checkAction1</item>
        <item>checkAction2</item>
        <item label="" />
        <item>radioAction1_1</item>
        <item>radioAction1_2</item>
        <item label="" />
        <item>radioAction2_1</item>
        <item>radioAction2_2</item>
        <item>radioAction2_3</item>
      </item>
      <item label="編集(&amp;E)">
        <item label="メニュー1(&amp;1)">
          <item>openAction</item>
          <item>newAction</item>
        </item>
        <item label="メニュー2(&amp;2)">
          <item>closeAction</item>
        </item>
      </item>
