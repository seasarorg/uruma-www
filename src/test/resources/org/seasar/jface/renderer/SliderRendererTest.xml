<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN" 
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="sliderRendererTest" extends="RendererTest.xml">
  <extends id="sliderRendererTest" property="title">
    SliderRendererTest
  </extends>
  <extends id="contents" />
  <window id="sliderRendererTest">

    <composite id="contents" type="box">
      <layoutData name="horizontalAlignment">FILL</layoutData>

      <control type="slider">
        <property name="max">100</property>
        <property name="min">0</property>
        <property name="selection">10</property>
        <property name="incriment">5</property>
      </control>

      <control type="slider">
        <property name="max">100</property>
        <property name="min">0</property>
        <property name="incriment">5</property>
        <property name="style">VARTICAL</property>
      </control>

    </composite>
  </window>
</template>
