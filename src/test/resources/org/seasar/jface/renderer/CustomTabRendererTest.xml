<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN" 
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="customTabRendererTest" extends="RendererTest.xml">
  <extends id="customTabRendererTest" property="title">
    CustomTabRendererTest
  </extends>
  <extends id="contents" />
  
  <window id="customTabRendererTest">
    <composite id="contents" type="box">
      <composite type="box" layout="fill"
        layoutParam="type=HORIZONTAL; marginHeight=10; marginWidth=10; spacing=10">
        <layoutData name="horizontalAlignment">FILL</layoutData>

        <control type="customTab">
          <property name="simpleStyle">true</property>
          <property name="text">tab1-1, tab1-2</property>
        </control>

        <control type="customTab">
          <property name="closeButton">true</property>
          <property name="text">tab2-1, tab2-2, tab2-3</property>
        </control>
      </composite>
    </composite>
  </window>
</template>
