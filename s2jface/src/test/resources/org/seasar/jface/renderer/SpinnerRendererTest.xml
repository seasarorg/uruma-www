<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN" 
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="spinnerRendererTest" extends="RendererTest.xml">
  <extends id="spinnerRendererTest" property="title">
    SpinnerRendererTest
  </extends>
  <extends id="contents" />
  <window id="spinnerRendererTest" layout="grid">
    <composite id="contents" type="box">
      <layoutData name="horizontalAlignment">FILL</layoutData>

      <control type="spinner">
        <property name="max">100</property>
        <property name="min">1</property>
        <property name="selection">20</property>
      </control>

      <control type="spinner">
        <property name="max">100</property>
        <property name="min">1</property>
      </control>
    </composite>
  </window>
</template>
