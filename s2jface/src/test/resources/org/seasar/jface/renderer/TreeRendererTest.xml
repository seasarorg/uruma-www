<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN"
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="treeRendererTest" extends="RendererTest.xml">
  <extends id="treeRendererTest" property="title">
    TreeRendererTest
  </extends>
  <extends id="contents" />
  <window id="treeRendererTest" layout="grid">
    <composite id="contents" type="box">
      <composite type="box" layout="row"
        layoutParam="type=HORIZONTAL; marginHeight=100; marginWidth=100; spacing=20">
        <layoutData name="horizontalAlignment">FILL</layoutData>
        <control type="tree">
          <property name="width">80</property>
          <property name="height">80</property>
          <item label="親">
            <item label="子">
              <item label="孫" />
            </item>
          </item>
        </control>

        <control type="tree">
          <property name="width">50</property>
          <property name="height">80</property>
          <item label="親">
            <item label="子" />
            <item label="子" />
          </item>
        </control>
      </composite>
    </composite>
  </window>
</template>
