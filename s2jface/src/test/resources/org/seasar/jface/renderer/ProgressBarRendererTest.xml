<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN" 
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="progressBarRendererTest" extends="RendererTest.xml">
  <extends id="progressBarRendererTest" property="title">
    ProgressBarRendererTest
  </extends>
  <extends id="contents" />

  <window id="progressBarRendererTest">
    <composite id="contents" type="box">
      <layoutData name="horizontalAlignment">FILL</layoutData>

      <control type="progressBar">
        <property name="max">100</property>
        <property name="min">0</property>
        <property name="selection">10</property>
      </control>

      <control type="progressBar" style="VERTICAL">
        <property name="max">100</property>
        <property name="min">0</property>
      </control>
      
      <control type="progressBar" style="VERTICAL, SMOOTH">
        <property name="max">100</property>
        <property name="min">0</property>
        <property name="selection">66</property>
      </control>
    </composite>

  </window>
</template>
