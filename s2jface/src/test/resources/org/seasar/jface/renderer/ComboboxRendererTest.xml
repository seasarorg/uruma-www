<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN"
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="comboboxRendererTest" extends="RendererTest.xml">
  <extends id="comboboxRendererTest" property="title">
    ComboboxRendererTest
  </extends>
  <extends id="contents" />

  <window id="comboboxRendererTest">
    <composite id="contents" type="box">
      <composite type="box" layout="row"
        layoutParam="type=HORIZONTAL; marginHeight=100; marginWidth=100; spacing=20">
        <layoutData name="horizontalAlignment">FILL</layoutData>

        <control type="combobox">
          <item label="Item1-1" />
          <item label="Item1-2" />
          <item label="Item1-3" />
        </control>

        <control type="combobox">
          <property name="style">DROP_DOWN</property>
          <item label="Item2-1" />
          <item label="Item2-2" />
          <item label="Item2-3" />
        </control>

        <control type="combobox">
          <property name="style">READ_ONLY</property>
          <item label="Item3-1" />
          <item label="Item3-2" />
          <item label="Item3-3" />
        </control>
      </composite>
    </composite>
  </window>
</template>
