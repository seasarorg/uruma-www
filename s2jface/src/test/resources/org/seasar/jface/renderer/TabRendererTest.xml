<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE template PUBLIC "-//SEASAR//DTD S2JFace 0.1//EN" 
	"http://www.seasar.org/dtd/s2jface01.dtd">
<template name="tabRendererTest" extends="RendererTest.xml">
  <extends id="tabRendererTest" property="title">
    TabRendererTest
  </extends>
  <extends id="contents" />
  <window id="tabRendererTest" layout="grid">
    <composite id="contents" type="box">
      <layoutData name="horizontalAlignment">FILL</layoutData>

      <control type="tab">
        <property name="text">tab1-1, tab1-2</property>
      </control>

      <control type="tab">
        <property name="text">tab2-1, tab2-2, tab2-3</property>
      </control>
    </composite>
  </window>
</template>
