<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://s2jface.sandbox.seasar.org">
  <window id="scaleRendererGUITest" title="ScaleRendererGUITest" defaultButtonId="okButton">
    <gridLayout numColumns="1">
      <gridData horizontalAlignment="FILL" />
    </gridLayout>

    <composite id="contents">
      <gridData horizontalAlignment="FILL" />
      <rowLayout type="HORIZONTAL" marginWidth="10" marginHeight="10"
        spacing="10">
      </rowLayout>
      <commonAttributes fontHeight="20" />

      <scale minimum="0" maximum="100" selection="10" toolTipText="ツールチップ\nテキスト"
        increment="5" />

      <scale style="VERTICAL" minimum="0" maximum="100" selection="20"
        increment="1" pageIncrement="10" />

      <scale style="VERTICAL" minimum="0" maximum="100" selection="66"
        increment="10" pageIncrement="20" />
    </composite>

    <label style="SEPARATOR, HORIZONTAL" />

    <composite>
      <fillLayout type="HORIZONTAL" spacing="5" />

      <button id="okButton" text="OK"></button>
      <button id="ngButton" text="NG"></button>
    </composite>
  </window>
</template>
